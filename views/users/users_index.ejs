<!DOCTYPE html>
<html>
<head>
    <title>User List</title>
</head>
<body>
    <%- include('../partials/_navbar.ejs') %>
    <h1>All Users</h1>
    <ul>
        <% users.forEach(user => { %>
            <li><a href="userslist/<%= user._id %>">
                <%= user.username %>
            </a>
            </li>
        <% }); %>
    </ul>
</body>
</html>
